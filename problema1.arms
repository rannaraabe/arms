mdc(a:int, b:int): int {
    res = 0 :int;
    if (b == 0) {
        res = a;
    } else {
        res = mdc(b, a % b);
    };
    return res;
};

struct oi {
    x :int;
    y :double;
};

main(){
    x = 2, y = 3.6 : double;
    c = 10 : int;
    result = x*x - y + c : double;
    print << mdc(4, 8);
    print << "O resultado eh:" << result;
}